{
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescript.tsx",
    "json",
    "javascriptreact",
    "typescriptreact",
    "css",
    "markdown",
    "less",
    "sass",
    "scss",
    "html",
    "vue"
  ],
  "coc.preferences.formatOnSaveFiletypesIgnore": ["iss"],
  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx",
    "vue"
  ],
  "typescript.validate.enable": false,
  "coc.preferences.diagnostic.virtualText": true,
  "prettier.disableLanguages": [],
  "eslint.autoFixOnSave":false 
}
